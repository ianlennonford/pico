pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--factory
--(c)2017 increment

function _init()
	cls()
	palt(15,true)--sets peach as the transparent color
 palt(0,false)--makes black a useable color
--global variables--
	state="create"
	step=0
	grav=0.2
--initialize game entities
	actors={}--table for all actors
	p={--player object
		state="normal",
  x=0,
  y=0,
  dx=0,
  dy=0,
  spd=1,
  climbspd=0.5,
  jumpspd=-2,
  facing=1,--1=right,-1=left
		onground=true,
		frame=1,
		idle={1},
		walk={1,2,3,4,5,2,6,7},
		jump={4},
		climb_idle={9},
		climb={8,9,10},
  draw=function(self)--draws the actor
  	if self.facing==-1 then--if facing left flip the sprite
   	spr(self.anim[self.frame],self.x,self.y,1,1,true,false)
   else
    spr(self.anim[self.frame],self.x,self.y)--otherwise draw it normally
   end
  end,
  update=function(self)
  	if(step%6==0)self.frame+=1
  	if self.frame>#self.anim then self.frame=1 end
--normal player state--
			if self.state=="normal" then
				if btn(2) then
					local u=mget((self.x)/8,(self.y)/8)
					if fget(u,1) then--ladder
						self.frame=1
						self.x=flr((self.x)/8)*8
						self.state="ladder"
						return
					end
				end
				if btn(3) then
					local u=mget((self.x)/8,(self.y+15)/8)
					if fget(u,2) then--climb down
						self.frame=1
						self.x=flr((self.x)/8)*8
						self.state="ladder"
						return
					end
				end
	--input for player movement--
				local startx=self.x
		--jump
  		if btnp(5) and self.onground then
					--sfx(0)*add back in jump noise
					self.dy=self.jumpspd
				end
		--left/right movement
  		if btn(0) or btn(1) then
  			if step%10==0 and self.onground then
  				--sfx(1)*not sure about this one
  			end
  			if btn(0) then self.facing=-1 end
  			if btn(1) then self.facing=1 end
  			self.anim=self.walk
  			self.dx=self.spd*self.facing
  		else
  			self.frame=1
  			self.anim=self.idle
  			self.dx=0
  		end
  	--move player left/right
  		self.x+=self.dx
  	--side collision
 	 	local xoffset=0
				if self.dx>0 then xoffset=7 end
  		local h=mget((self.x+xoffset)/8,(self.y+7)/8)
				if fget(h,0) then
					self.x=startx
				end
			--add gravity
				self.dy+=grav
			--move player up/down
				self.y+=self.dy
			--floor collision
				local v=mget((self.x+4)/8,(self.y+8)/8)
				self.onground=false
				if self.dy>=0 then--if moving down
					if fget(v,0) then
						self.y=flr((self.y)/8)*8
						self.dy=0
						self.onground=true
					end
				end
		--ceiling collision
				v=mget((self.x+4)/8,(self.y)/8)
				if self.dy<=0 then--if moving up
					if fget(v,0) then
						self.y=flr((self.y+8)/8)*8
						self.dy=0
					end
				end
			--jump animation
  		if not self.onground then
  			self.frame=1
  			self.anim=self.jump
  		end
 --ladder state
  	elseif self.state=="ladder" then
  		if btn(2) then--climb
  			self.anim=self.climb
  			self.y-=self.climbspd
  			local u=mget((self.x)/8,(self.y+15)/8)
					if fget(u,2) then--ladder top
  				self.frame=1
  				self.state="normal"
  			end
  		elseif btn(3) then--descend
  			self.anim=self.climb
  			self.y+=self.climbspd
  			local u=mget((self.x)/8,(self.y+15)/8)
					if fget(u,0) then
						self.frame=1
						self.state="normal"
					end
  		elseif btn(5) then--jump off ladder
  			self.frame=1
  			self.state="normal"
  		else
  			self.frame=1
  			self.anim=self.climb_idle
  		end
  	end
  end,
	}
	add(actors,p)--add player to actors list
end

function _update60()
	step+=1
	
	if state=="play" then
		foreach(actors,function(a)
			a:update()
		end)
	end
end

function _draw()
	cls()
	
	if state=="create" then
		map(0,0,0,0,16,16)--draw map
		--chack map and create actors
		for y=0,16 do
			for x=0,16 do
				local s=mget(x,y)
				if(fget(s,7))then--if an actor is here
					p.x=x*8
					p.y=y*8
					p.anim=p.idle
					mset(x,y,0)
				end
			end
		end
		state="play"
	end
	
	if state=="play" then
		map(0,0,0,0,16,16)--draw map
		foreach(actors,function(a)
			a:draw()
		end)
	end
end
